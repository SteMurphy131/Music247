<p id="notice"><%= notice %></p>

<h1>Listing User Labels</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Label</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_labels.each do |user_label| %>
      <tr>
        <td><%= user_label.user_id %></td>
        <td><%= user_label.label_id %></td>
        <td><%= link_to 'Show', user_label %></td>
        <td><%= link_to 'Edit', edit_user_label_path(user_label) %></td>
        <td><%= link_to 'Destroy', user_label, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User label', new_user_label_path %>
